{
  "name": "stock-options-calculator",
  "version": "0.0.1",
  "description": "Common calculations on stock options",
  "main": "index.js",
  "type": "commonjs",
  "types": "index.d.ts",
  "scripts": {
    "lint": "standard --verbose | snazzy",
    "lint:fix": "standard --fix",
    "test": "tap -J test/*.test.js",
    "test:ci": "npm run lint && npm test -- --coverage-report=lcovonly && npm run test:typescript",
    "test:typescript": "tsd"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VBetsun/stock-options-calculator.git"
  },
  "keywords": [
    "stock",
    "options",
    "calculator"
  ],
  "author": "Vlad Betsun <vlad.betcun@gmail.com>",
  "contributors": [
    {
      "name": "Vlad Betsun",
      "email": "vlad.betcun@gmail.com",
      "author": true
    }
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/VBetsun/stock-options-calculator/issues"
  },
  "homepage": "https://github.com/VBetsun/stock-options-calculator",
  "engines": {
    "node": ">=10.16.0"
  },
  "devDependencies": {
    "snazzy": "^9.0.0",
    "standard": "^16.0.3",
    "tap": "^15.0.6",
    "tsd": "^0.15.0",
    "typescript": "^4.2.4"
  },
  "standard": {
    "ignore": [
      "index.d.ts"
    ]
  },
  "tsd": {
    "directory": "test"
  }
}
